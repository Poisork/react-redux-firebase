{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "standard"
  ],
  "globals": {
    "process": "readonly"
  },
  "rules": {
    "react/jsx-uses-react": 1,
    "import/no-duplicates": 0,
    "camelcase": 0,
    "react/display-name": 0
  }
}